{
  "name": "community-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "tsc -p tsconfig.server.json && PORT=${PORT:-3000} node server.mjs",
    "build": "NODE_ENV=production cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" NEXT_DISABLE_SENTRY=1 next build --no-lint",
    "build:static": "NODE_ENV=production cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" NEXT_DISABLE_SENTRY=1 SKIP_DATABASE_CALLS=true next build --no-lint",
    "build:bypass": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" NEXT_DISABLE_SENTRY=1 NEXT_TELEMETRY_DISABLED=1 next build",
    "build:simple": "NODE_ENV=production SKIP_TYPE_CHECKING=true cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" NEXT_TELEMETRY_DISABLED=1 next build --no-lint --no-types",
    "build:server": "tsc -p tsconfig.server.json",
    "start:next": "next start -p ${PORT:-3001}",
    "start": "NODE_ENV=production PORT=${PORT:-3000} node server.mjs",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "seed": "npx ts-node prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "cron:48h": "tsc -p tsconfig.server.json && node -e \"import('./dist/lib/cronService.js').then(m => m.runJobManually('48_HOUR_REMINDER'))\"",
    "cron:24h": "tsc -p tsconfig.server.json && node -e \"import('./dist/lib/cronService.js').then(m => m.runJobManually('24_HOUR_REMINDER'))\"",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:prod": "NODE_ENV=production npm run deploy",
    "clean": "rm -rf dist .next",
    "prepare": "npm run build:server",
    "db:fix": "chmod +x scripts/apply-missing-fields.js && node scripts/apply-missing-fields.js",
    "db:verify": "chmod +x scripts/verify-schema.js && node scripts/verify-schema.js",
    "predev": "npm run db:verify",
    "prebuild": "npm run db:verify",
    "analyze": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" ANALYZE=true NEXT_IGNORE_ESLINT=1 next build",
    "find-unused": "node scripts/find-unused-imports.js",
    "optimize": "npm run find-unused && npm run analyze"
  },
